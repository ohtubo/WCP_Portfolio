<h2>プロフィール編集</h2>
<%= form_with model:@user, local:true do |f| %>
  <label>ユーザーネーム</label>
  <%= f.text_field :name, autofocus: true%></br>
  <label>自己紹介</label>
  <%= f.text_area :self_introduction %></br>
  <label>アイコン画像</label>
  <%= f.attachment_field :icon_image %></br>
  <%= f.submit "プロフィール登録" %>
<% end %>
